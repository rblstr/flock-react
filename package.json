{
  "name": "flock-react",
  "version": "0.0.1",
  "description": "A port of Flock to React",
  "main": "index.js",
  "scripts": {
    "prestart": "mkdir -p dist && browserify -d index.js -o dist/index.js -t [ babelify --presets [ es2015 react ] ]",
    "start": "static",
    "watch": "watchify -d src/main.js -o dist/index.js -t [ babelify --presets [ es2015 react ] --plugins [ transform-object-rest-spread ] ]"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rblstr/flock-react.git"
  },
  "author": "Rob Lester <rblestr@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "immutable": "^3.8.1",
    "jquery": "^3.1.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "semantic-ui": "^2.2.2",
    "urijs": "^1.18.1",
    "whatwg-fetch": "^1.0.0",
    "youtube-player": "^3.0.4"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "node-static": "^0.7.8",
    "watchify": "^3.7.0"
  }
}
